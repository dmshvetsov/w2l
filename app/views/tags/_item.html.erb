<li class="grid <%= chart_class_item %>">
  <div class="grid__item one-twelfth">
    <%= index + 1 %>.
  </div>
  <div class="grid__item chart-item-love-hate one-twelfth">
    <%= link_to tag.likes.count, like_tag_url(liked_tag_id: tag.id, user_who_liked: current_user.id) %> / 0
  </div>
  <h1 class="grid__item chart-item-name five-twelfths">
    <%= tag.name %>
  </h1>
  <div class="grid__item chart-item-places five-twelfths">
    there are no one places with this tag
  </div>
</li>